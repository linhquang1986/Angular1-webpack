  "devDependencies": {
    "angular-mocks": "1.6.6",
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "bower": "1.8.2",
    "clean-webpack-plugin": "0.1.17",
    "css-loader": "0.28.7",
    "expose-loader": "0.7.3",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.4",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "http-server": "0.10.0",
    "istanbul-instrumenter-loader": "3.0.0",
    "jasmine-core": "2.4.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.4",
    "node-sass": "4.5.3",
    "null-loader": "0.1.1",
    "path": "0.12.7",
    "postcss-loader": "2.0.6",
    "protractor": "5.1.2",
    "sass-loader": "6.0.6",
    "standard": "10.0.3",
    "style-loader": "0.19.0",
    "uglifyjs-webpack-plugin": "0.4.6",
    "url-loader": "0.6.2",
    "webpack": "3.6.0",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.9.1",
    "webpack-merge": "4.1.0"
  },
  "scripts": {
    "start:analyzer": "npm --analyzer=true run start:dev",
    "start:dev":
      "npm install && webpack-dev-server --progress --config webpack.dev.js",
    "start:dev:vagrant":
      "npm install && webpack-dev-server  --host 0.0.0.0 --public 10.10.10.61:8000 --watch-poll --progress --config webpack.dev.js",
    "build:dev": "npm install && webpack --progress --config webpack.dev.js",
    "build:prod": "npm install && webpack --progress --config webpack.prod.js",
    "start:server": "http-server -p 8000 ./dist",
    "update-deps": "npm update",
    "start": "npm run start:dev:vagrant",
    "pretest": "npm install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js"
  },
  "standard": {
    "globals": ["angular", "inject"],
    "env": ["jasmine", "mocha", "protractor", "angular"]
  },
  "dependencies": {
    "angular": "1.6.6",
    "angular-route": "1.6.6",
    "angular-ui-bootstrap": "2.5.0",
    "autoprefixer": "7.1.4",
    "events": "1.1.1",
    "jquery": "3.2.1",
    "lodash": "4.17.4"
  }
}
